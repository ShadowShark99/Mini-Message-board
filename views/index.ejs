<html>
  <head>
    <title>Homepage</title>
    <link rel="stylesheet" href="/styles.css" />
  </head>
  <body>
    <h1><%= title %></h1>
    <ul>
      <% for (let i = 0; i < messages.length; i++) { %>
      <div>
        <h4>
          <span> <%= messages[i].user %> </span>
        </h4>
        <div>
          <span> <%= messages[i].text %> </span>
        </div>
        <div>
          <span> <%= messages[i].added %> </span>
        </div>
      </div>
      <% } %>
    </ul>
    <a href="/new">Send a Message!</a>
  </body>
</html>
